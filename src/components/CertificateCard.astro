---
import type { CollectionEntry } from 'astro:content';

export interface Props {
    certificate: CollectionEntry<"certifications">
}

const {certificate} = Astro.props

---

<div>
    <h4>{certificate.data.rank} {certificate.data.title}</h4>
    <div>
      <p>{certificate.data.institution} ({certificate.data.obtentionYear})</p>
      </div>
  </div>